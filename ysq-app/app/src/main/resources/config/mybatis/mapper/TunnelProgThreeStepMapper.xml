<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cmct.ysq.mapper.TunnelProgThreeStepMapper">


    <select id="getConstructProgressHisInfo" resultType="com.cmct.ysq.model.vo.TunnelProgThreeStepVo">

        select
          tpts.*,su.username as createByAccount
        from
          t_tunnel_prog_three_step tpts
        left join t_sys_user su on su.user_id = tpts.create_by
        where
        tpts.prg_id in
        <foreach item="item" index="index" collection="constructProgressIds" open="(" separator="," close=")">
            #{item}
        </foreach>
        and
          tpts.delete_status = 0
        order by
        create_date
        desc


    </select>

</mapper>